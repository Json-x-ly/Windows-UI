<Window x:Class="Windows_UI_APP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="800">
    <Grid Margin="0,0,2,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200*"/>
            <ColumnDefinition Width="600*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>
        <Image HorizontalAlignment="Left" Height="626" VerticalAlignment="Top" Width="189" Margin="10,10,0,0"/>
        <Button x:Name="_Button_01" Content="Button" Margin="10,0,43,10" Click="Generate_QR_Codes" Height="50" VerticalAlignment="Bottom"/>
        <TextBox x:Name="_TextBox_01" HorizontalAlignment="Left" Height="23" Margin="43,9,0,0" TextWrapping="Wrap" Text="Enter Large Text" VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Label" VerticalAlignment="Top" Height="16" Width="28"/>
    </Grid>
</Window>
